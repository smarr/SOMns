ProxyAdd = Benchmark (
    | proxy calc |
    
    initialize = (
        calc := Calculator new: 5.
        proxy := Proxy new: calc
    )

    benchmark = (
        calc initializeWith: 5.
    
        1 to: 20000 do: [ :i | 
            proxy inc: #once
        ].
    
        calc a = 20005 ifFalse: [ 'Benchmark failed with wrong result' println. calc a println. ]
    )
    
    ----
    
    new = (
        ^ super new initialize
    )
)
